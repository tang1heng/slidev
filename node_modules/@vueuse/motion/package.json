{
  "name": "@vueuse/motion",
  "version": "2.0.0-beta.24",
  "description": "ðŸ¤¹ Vue Composables putting your components in motion",
  "repository": "https://github.com/vueuse/motion",
  "bugs": {
    "url": "https://github.com/vueuse/motion/issues"
  },
  "homepage": "https://github.com/vueuse/motion#readme",
  "author": "YaeÌˆl GUILLOUX <yael.guilloux@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "hook",
    "motion",
    "animation",
    "v-motion",
    "popmotion-vue"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "dev": "nr dev:vite",
    "release": "release-it",
    "lint": "eslint --ext .js,.vue,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.vue,.ts,.tsx . --fix",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test": "pnpm test:unit && pnpm test:coverage",
    "prepare": "nr prepare:nuxt",
    "__": "__",
    "dev:nuxt": "(cd playgrounds/nuxt && nr dev:nuxt)",
    "build:nuxt": "(cd playgrounds/nuxt && nr build:nuxt)",
    "generate:nuxt": "(cd playgrounds/nuxt && nr preview:nuxt)",
    "dev:ssg": "(cd playgrounds/vite-ssg && nr dev:ssg)",
    "build:ssg": "(cd playgrounds/vite-ssg && nr build:ssg)",
    "preview:ssg": "(cd playgrounds/vite-ssg && nr preview:ssg)",
    "dev:vite": "(cd playgrounds/vite && nr dev:vite)",
    "build:vite": "(cd playgrounds/vite && nr build:vite)",
    "preview:vite": "(cd playgrounds/vite && nr preview:vite)",
    "dev:docs": "(cd docs && nr dev:docs)",
    "build:docs": "(cd docs && nr build:docs)",
    "preview:docs": "(cd docs && nr preview:docs)",
    "prepare:nuxt": "(cd playgrounds/nuxt && nr prepare:nuxt)"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@vueuse/core": "^9.3.1",
    "@vueuse/shared": "^9.3.1",
    "csstype": "^3.1.1",
    "framesync": "^6.1.2",
    "popmotion": "^11.0.5",
    "style-value-types": "^5.1.2"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "optionalDependencies": {
    "@nuxt/kit": "^3.0.0-rc.12"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.0.0-rc.12",
    "@antfu/eslint-config": "^0.27.0",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "@vitest/coverage-c8": "^0.24.3",
    "@vue/test-utils": "^2.1.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "happy-dom": "^7.6.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "unbuild": "^0.9.4",
    "vite": "3.1.8",
    "vitest": "^0.24.3",
    "vue": "^3.2.41",
    "yorkie": "^2.0.0",
    "@antfu/ni": "^0.18.2"
  },
  "resolutions": {
    "@nuxt/content": "2.1.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "@types/react",
        "react",
        "react-dom",
        "webpack",
        "postcss",
        "tailwindcss",
        "vue",
        "axios"
      ]
    }
  },
  "release-it": {
    "hooks": {
      "before:init": [
        "pnpm build"
      ]
    },
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
